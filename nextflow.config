// Define params
params.in = "$baseDir/sequence.fa"

// Define HPC profiles to run with job scheduler
profiles {
  slurm {
    process {
      executor = 'slurm'
      queue = 'work'
      clusterOptions = '--account=pawsey1227'
    }
  }
  
  pbs {
    process {
      executor = 'pbs'
      queue = 'normal'
      clusterOptions = '-P ad78'
    }
  }
}